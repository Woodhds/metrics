{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ElasticOptions": {
    "Host": "http://192.168.99.105:31200"
  },
  "Token": {
    "Value": "bbb703847bc50b39020e68b56682e32341f4c3f822c6688003245bebd124b645b13b320bf6b0a41c75bbd"
  },
  "VKApiUrls": {
    "Domain": "https://api.vk.com/method/",
    "MainDomain": "https://vk.com/",
    "Wall": "wall.get",
    "WallSearch": "wall.search",
    "Repost": "wall.repost",
    "WallGetById": "wall.getById",
    "GroupJoin": "groups.join",
    "UserInfo":  "users.get",
    "Groups": "groups.get",
    "LeaveGroup": "groups.leave",
    "Like": "likes.add"
  },
  "CompetitionOptions": {
    "CityId": "97"
  },
  "NLog": {
    "extensions": [
      {
        "assembly": "NLog.Targets.ElasticSearch"
      }
    ],
    "targets": {
      "main": {
        "type": "BufferingWrapper",
        "flushTimeout": 5000,
        "target": {
          "type": "ElasticSearch",
          "uri": "${configsetting:name=ElasticOptions.Host}",
          "layout": "${message}"
        }
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "main"
      }
    ]
  }
}