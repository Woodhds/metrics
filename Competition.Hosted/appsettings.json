{
  "NLog": {
    "extensions": [
      {
        "assembly": "NLog.Targets.ElasticSearch"
      }
    ],
    "targets": {
      "main": {
        "type": "BufferingWrapper",
        "flushTimeout": 5000,
        "target": {
          "type": "ElasticSearch",
          "uri": "${configsetting:name=ElasticOptions.Host}"
        }
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Warn",
        "writeTo": "main"
      }
    ]
  }
}