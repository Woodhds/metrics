{
  "NLog": {
    "extensions": [
      {
        "assembly": "NLog.Targets.ElasticSearch"
      }
    ],
    "targets": {
      "main": {
        "type": "BufferingWrapper",
        "flushTimeout": 5000,
        "target": {
          "type": "ElasticSearch",
          "uri": "${configsetting:name=ElasticOptions.Host}"
        }
      },
      "logConsole": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Off",
        "writeTo": "main"
      },
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "logConsole"
      }
    ]
  }
}
