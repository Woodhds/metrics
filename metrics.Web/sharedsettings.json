{
  "FrontendUrl": "http://localhost:4200",
  "AllowedHosts": "*",
  "JwtOptions": {
    "key": "62AF691B3FE1FCA6BE1FED6EE12D935A67ED66EE07D46483CBB79707B4CFA8B1",
    "Issuer": "metrics",
    "Audience": "metrics"
  },
  "CacheOptions": {
    "Configuration": "localhost:6379,password=password",
    "Instance": "UserTokens"
  },
  "ConsulConfig": {
    "Address": "http://localhost:8500"
  },
  "AmqpOptions": {
    "Host": "localhost:5672",
    "UserName": "user",
    "Password": "password"
  },
  "NLog": {
    "extensions": [
      {
        "assembly": "NLog.Targets.ElasticSearch"
      }
    ],
    "targets": {
      "main": {
        "type": "BufferingWrapper",
        "flushTimeout": 5000,
        "target": {
          "type": "ElasticSearch",
          "uri": "${configsetting:name=ElasticOptions.Host}"
        },
        "fields": [
          {
            "name": "StackTrace",
            "layout": "${exception:format=ToString,StackTrace}${newline}"
          }
        ]
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Error",
        "writeTo": "main"
      }
    ]
  },
  "ElasticOptions": {
    "Host": "http://localhost:9200"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}